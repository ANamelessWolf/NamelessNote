{
  "name": "namelessnote-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "dev:detached": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up -d --build",
    "down:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "prod": "docker compose up --build",
    "prod:detached": "docker compose up -d --build",
    "down:prod": "docker compose down"
  }
}
